/* sample.C - Sample Main program for SPEDE 	Aug */

#include <spede/stdio.h>		// For printf()
#include <spede/flames.h>		// For cons_printf()

int main (void)
{
	long i;

	i = 111;
	while (i < 116)
	{
		printf( "%d Hello world %d \nECS", i, 2 * i);
		cons_printf( "--> Hello world <--\nCPE/CSC" );  //target printf
		i = i + 1;
	}
	return 0;
}     // end main()
